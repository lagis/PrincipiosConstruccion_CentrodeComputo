<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="centro.de.computo.MenuController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="lUsuario" layoutX="40.0" layoutY="28.0" text="Label" />
            <Label fx:id="lPuesto" layoutX="40.0" layoutY="70.0" text="Label" />
            <Button fx:id="bttHardware" layoutX="255.0" layoutY="75.0" mnemonicParsing="false" onAction="#abrirInventarioHardware" text="Consultar hardware" />
            <Button fx:id="bttPersonal" layoutX="251.0" layoutY="151.0" mnemonicParsing="false" onAction="#clickAdministrarPersonal" text="Administrar personal" />
            <Button fx:id="bttEquipo" layoutX="251.0" layoutY="222.0" mnemonicParsing="false" onAction="#prestarEquipo" prefHeight="26.0" prefWidth="126.0" text="Prestar Equipo" />
         </children>
    </AnchorPane>
  </children>
</VBox>
