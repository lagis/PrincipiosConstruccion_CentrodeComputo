<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="centro.de.computo.MenuController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="bttHardware" layoutX="251.0" layoutY="98.0" mnemonicParsing="false" onAction="#abrirInventarioHardware" prefHeight="26.0" prefWidth="138.0" text="Consultar hardware" />
            <Button fx:id="bttPersonal" layoutX="251.0" layoutY="187.0" mnemonicParsing="false" onAction="#clickAdministrarPersonal" prefHeight="26.0" prefWidth="138.0" text="Administrar personal" />
            <Button fx:id="bttEquipo" layoutX="251.0" layoutY="280.0" mnemonicParsing="false" onAction="#prestarEquipo" prefHeight="26.0" prefWidth="138.0" text="Prestar Equipo" />
            <Button fx:id="bttReporte" layoutX="251.0" layoutY="350.0" mnemonicParsing="false" onAction="#generarReporte" prefHeight="26.0" prefWidth="138.0" text="Generar reporte" />
         </children>
    </AnchorPane>
  </children>
</VBox>
